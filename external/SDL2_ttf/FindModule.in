# Set our @EXTERNAL_OPTION_DIR@_LIBRARIES value
set(@EXTERNAL_NAME@_LIBRARIES @FIND_LIBRARIES@)

# Set our @EXTERNAL_OPTION_DIR@_LIBRARY value
set(@EXTERNAL_NAME@_LIBRARY @FIND_LIBRARIES@)

# Set our @EXTERNAL_OPTION_DIR@_FOUND value
set(@EXTERNAL_NAME@_FOUND TRUE)

# Set our @EXTERNAL_OPTION_DIR@_LOCAL value
set(@EXTERNAL_NAME@_LOCAL TRUE)

